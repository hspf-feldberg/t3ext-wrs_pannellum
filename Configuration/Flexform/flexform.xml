<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <Einstellungen>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Einstellungen</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.autoload>
                        <TCEforms>
                            <label>Autoload</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.autoload>
                    <settings.test>
                        <TCEforms>
                            <label>Test</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.test>
                    <settings.preview>
                        <TCEforms>
                            <label>Preview</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>sys_file</allowed>
                                <size>10</size>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <show_thumbs>1</show_thumbs>
                                <appearance>
                                    <elementBrowserType>file</elementBrowserType>
                                    <elementBrowserAllowed>gif,jpg,jpeg,tif,tiff,bmp,pcx,tga,png,pdf,ai</elementBrowserAllowed>
                                </appearance>
                            </config>
                        </TCEforms>
                    </settings.preview>
                </el>
            </ROOT>
        </Einstellungen>
    </sheets>
</T3DataStructure>