<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <Einstellungen>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Einstellungen</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.autoload>
                        <TCEforms>
                            <label>Autoload</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.autoload>
                    <settings.autorotate>
                        <TCEforms>
                            <label>Autorotate</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.autorotate>
                    <settings.friction>
                        <TCEforms>
                            <label>Friction</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.friction>
                    <settings.draggable>
                        <TCEforms>
                            <label>Draggable</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.draggable>
                    <settings.ui>
                        <TCEforms>
                            <label>Show UI</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.ui>
                </el>
            </ROOT>
        </Einstellungen>
    </sheets>
</T3DataStructure>